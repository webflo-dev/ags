{
  "root": true,
  "env": {
    "es2021": true
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "globals": {
    "ags": "readonly",
    "ARGV": "readonly",
    "imports": "readonly",
    "print": "readonly",
    "console": "readonly",
    "logError": "readonly"
  },
  "rules": {}
}
