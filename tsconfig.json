{
  "compilerOptions": {
    "target": "ESNext",
    // "module": "ES2022",
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "lib": ["dom", "ES2022"],
    "allowJs": false,
    "checkJs": false,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "typeRoots": ["./types"],
    "skipLibCheck": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      // "@types/*": ["./types/*"],
      "@services": ["./src/services/index.ts"],
      "@modules/*": ["./src/modules/*"],
      // "@icons": ["./src/icons.ts"],
      "@utils": ["./src/utils.ts"],
      "@widgets": ["./src/widgets/index.ts"]
    }
  }
  //"include": ["./src/**/*.ts*"]
  // "include": ["./src/**/*.ts*", "./types/**/ags.d.ts"]
}
